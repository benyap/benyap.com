    "@svgr/core": "8.1.0",
    "@svgr/plugin-jsx": "8.1.0",
    "@svgr/plugin-svgo": "8.1.0",
    "@types/node": "22.4.0",
    "@types/react": "18.3.3",
    "chokidar-cli": "3.0.0",
    "fast-glob": "3.3.2",
    "react": "18.3.1",
    "ts-node": "10.9.2"
  }
