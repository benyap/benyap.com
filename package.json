{
  "name": "benyap.com",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "packageManager": "yarn@1.22.15",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "gen": "HYGEN_TMPLS=templates hygen",
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write '**/*.{ts,tsx,md}'"
  },
  "devDependencies": {
    "hygen": "6.2.0",
    "prettier": "2.5.1",
    "turbo": "1.1.5"
  }
}
